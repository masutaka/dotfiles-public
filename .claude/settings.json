{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
    "SHELL": "/bin/zsh"
  },
  "permissions": {
    "allow": [
      "Bash(curl:*)",
      "Bash(find:*)",
      "Bash(gemini:*)",
      "Bash(grep:*)",
      "Bash(jq:*)",
      "Bash(rg:*)",
      "Bash(wget:*)",
      "Read(.env.example)",
      "mcp__github__get_file_contents",
      "mcp__github__get_pull_request",
      "mcp__github__list_commits",
      "mcp__github__list_pull_requests",
      "mcp__github__search_pull_requests"
    ],
    "deny": [
      "Bash(curl:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git rebase:*)",
      "Bash(git reset:*)",
      "Bash(nc:*)",
      "Bash(rm -rf .git)",
      "Bash(rm -rf /)",
      "Bash(rm -rf /*)",
      "Bash(rm -rf ~)",
      "Bash(sudo:*)",
      "Bash(wget:*)",
      "Read(.env)",
      "Read(.envrc)",
      "Write(.env*)"
    ]
  },
  "enableAllProjectMcpServers": false,
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.js"
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754037400924
  }
}